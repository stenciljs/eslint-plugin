{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "allowUnreachableCode": false,
    "declaration": false,
    "experimentalDecorators": true,
    "jsx": "react",
    "jsxFactory": "h",
    "lib": [
      "es2017.intl",
      "es5",
      "dom",
      "scripthost",
      "es2017",
      "es2019",
      "es2022"
    ],
    "module": "es2022",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "resolveJsonModule": true,
    "strict": true,
    "strictNullChecks": true,
    "target": "es2022"
  },
  "include": [
    "../rules",
    "types/jsx.d.ts"
  ]
}
